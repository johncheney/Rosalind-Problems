


def transcribe(n):
    for letter in n:
        if letter == 'A':
            return "A"
        elif letter == 'C': 
            return "C"
        elif letter == 'T': 
            return "U" 
        elif letter == 'G': 
            return "G"
   
   
   
transcribe("GATGGAACTTGACTACGTAAATT")





