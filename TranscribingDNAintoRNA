

def transcribe(n):
    for letter in n:
        if letter == 'A':
            print( 'A', end = ''),
        elif letter == 'C': 
            print('C', end = ''),
        elif letter == 'T': 
            print( 'U', end = ''),
        elif letter == 'G': 
            print('G', end = '')
        
   
transcribe("GATGGAACTTGACTACGTAAATT")





