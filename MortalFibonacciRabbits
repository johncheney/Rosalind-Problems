
def rabbitcounter(n,k, m):          # Describe the fxï½Ž
    p = 1                           #Initalize vars 
    o = 1 
    pairs = [1,1,]                  #Initalize lists 
    for i in range(3,n):           #loop through 
        o = o + p*k                 #parents give rise to offspring
        p = o - p*k                 #subtract offspring from total to get parents 
        pairs.append(o+p)           #append to a list for ease 
        
        print('parents =' ,p)
        print('offspring =' ,o)
        print('pairs =' ,pairs)
        
        print('  ')
       # print('pairs' = pairs)


rabbitcounter(6,1,3)

#desired output 4 
